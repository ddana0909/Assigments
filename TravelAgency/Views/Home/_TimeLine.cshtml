@model IEnumerable<TravelAgency.HelperClasses.TripTimelineElement>
@section scripts
{
    <script>
        $(function() {
            $("[data-toggle='tooltip']").tooltip();
        });
    </script>
}
<div class="progress">
    
    @foreach (var item in Model) {
        
        if (@item.NotPlanned == true)
         {
             <div class="@Html.DisplayFor(modelItem => item.CssClass)" style="width: @item.Percentage%">
                 @Html.DisplayFor(modelItem => item.StartDate)- @Html.DisplayFor(modelItem => item.FinishDate) 
                 Not planned
                 <span class="sr-only"></span>
             </div>
         }
        if (@item.NotPlanned==false)

         {
             <div 
                                data-toggle="tooltip"
                                data-placement="bottom"
                                title=""
                                data-original-title="bjcakbnlvnl"
                 class="@Html.DisplayFor(modelItem => item.CssClass)" style="width: @item.Percentage%">
                 @Html.DisplayFor(modelItem => item.StartDate)- @Html.DisplayFor(modelItem => item.FinishDate)
                 <span class="sr-only"></span> 
             </div>
         }
    }
</div>


